<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="anchorPane" prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="memberapp.view.EditProfileController">
   <VBox fx:id="vbox" alignment="CENTER" prefHeight="506.0" prefWidth="700.0" spacing="10.0">
      <children>
         <HBox fx:id="hbox0" alignment="CENTER" prefHeight="60.0" prefWidth="200.0" spacing="200.0">
            <children>
               <Label fx:id="header" alignment="CENTER" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefHeight="35.0" text="PROFILE">
                  <font>
                     <Font size="30.0" />
                  </font>
               </Label>
            </children>
            <padding>
               <Insets bottom="20.0" />
            </padding>
            <VBox.margin>
               <Insets bottom="15.0" />
            </VBox.margin>
         </HBox>
         <HBox fx:id="hbox1" alignment="CENTER" prefHeight="40.0" prefWidth="350.0" spacing="10.0">
            <Label fx:id="firstNameLbl" minWidth="-Infinity" prefHeight="50.0" prefWidth="120.0" text="First name" />
            <TextField fx:id="firstNameField" maxHeight="30.0" maxWidth="300.0" minHeight="-Infinity" prefHeight="30.0" prefWidth="200.0" HBox.hgrow="NEVER" />
         </HBox>
         <HBox fx:id="hbox2" alignment="CENTER" prefHeight="40.0" prefWidth="350.0" spacing="10.0">
            <Label fx:id="lastNameLbl" minWidth="-Infinity" prefHeight="50.0" prefWidth="120.0" text="Last name" />
            <TextField fx:id="lastNameField" maxHeight="30.0" maxWidth="300.0" minHeight="-Infinity" prefHeight="30.0" prefWidth="200.0" />
         </HBox>
         <HBox fx:id="hbox3" alignment="CENTER" prefHeight="40.0" prefWidth="350.0" spacing="10.0">
            <Label fx:id="usernameLbl" minWidth="-Infinity" prefHeight="50.0" prefWidth="120.0" text="Username" />
            <TextField fx:id="usernameField" maxHeight="30.0" maxWidth="300.0" minHeight="-Infinity" prefHeight="30.0" prefWidth="200.0" />
         </HBox>
         <HBox fx:id="hbox5" alignment="CENTER" prefHeight="40.0" prefWidth="350.0" spacing="10.0">
            <Label fx:id="emailLbl" minWidth="-Infinity" prefHeight="50.0" prefWidth="120.0" text="Email Address" />
            <TextField fx:id="emailField" maxHeight="30.0" maxWidth="300.0" minHeight="-Infinity" prefHeight="30.0" prefWidth="200.0" />
         </HBox>
         <HBox fx:id="hbox6" alignment="CENTER" prefHeight="40.0" prefWidth="350.0" spacing="10.0">
            <Label fx:id="phoneLbl" minWidth="-Infinity" prefHeight="50.0" prefWidth="120.0" text="Phone" />
            <TextField fx:id="phoneField" maxHeight="30.0" maxWidth="300.0" minHeight="-Infinity" prefHeight="30.0" prefWidth="200.0" />
         </HBox>
         <HBox fx:id="hbox7" alignment="CENTER" prefHeight="40.0" prefWidth="350.0" spacing="10.0">
            <Label fx:id="addressLbl" minWidth="-Infinity" prefHeight="50.0" prefWidth="120.0" text="Residential Address" />
            <TextField fx:id="addressField" maxHeight="30.0" maxWidth="300.0" minHeight="-Infinity" prefHeight="30.0" prefWidth="200.0" />
         </HBox>
         <Button fx:id="changePswrdBtn" mnemonicParsing="false" onAction="#changePassword" prefHeight="30.0" prefWidth="200.0" text="Change password" />
         <Button fx:id="saveBtn" mnemonicParsing="false" onAction="#saveChanges" prefHeight="30.0" prefWidth="200.0" text="Save Changes">
            <VBox.margin>
               <Insets top="5.0" />
            </VBox.margin>
         </Button>
      </children>
   </VBox>
</AnchorPane>
